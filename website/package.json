    "browser-sync": "^2.24.6",
    "clean-css": "^4.2.1",
    "css-mqpacker": "^7.0.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^7.0.1",
    "gulp-cached": "^1.1.1",
    "gulp-exec": "^3.0.2",
    "gulp-htmlmin": "^4.0.0",
    "gulp-imagemin": "^4.1.0",
    "gulp-postcss": "^8.0.0",
